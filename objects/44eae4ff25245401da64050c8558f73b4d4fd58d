return {

    name      = "basic pdf specific tagging : itemgroup",
    version   = "1.00",
    author    = "Hans Hagen & Mikael Sundqvist",
    copyright = "ConTeXt development team",

    mapping   = {

        itemgroup   = { pdf = "L"         },
        item        = { pdf = "LI"        },
        itemtag     = { pdf = "Lbl"       },
        itemcontent = { pdf = "LBody"     },
        itemhead    = { pdf = "NonStruct" },
        itembody    = { pdf = "NonStruct" },

    },

}
