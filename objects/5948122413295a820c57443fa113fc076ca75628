      \ConfigureEnv{tabular}{}{}{}{}{}{}% 
      \ConfigureEnv{center}{}{}{}{}}
   {\ifvmode \IgnorePar\fi \EndP
%     \HCode{</div>}%
    \egroup}
   {} {}
