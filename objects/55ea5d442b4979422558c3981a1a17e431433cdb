\fi
\@tempa{\make@newop@cmd{#1}
    {\@operator@robust@def#1{\mathchoice{}{}{}{}\@init@bfopfont
      {\@bfop@choices{#2}}}}}
\@operator@robust@def\DeclareMathOperator{\relax\@ifstar
  \@operatorlim\@operatornolim}
