	\DeclareDocumentCommand{\e}{O{#1} m}{\sde[##1]{##2}}
	\DeclareDocumentCommand{\E}{O{#1} m}{\sdE[##1]{##2}}
	
	\DeclareDocumentCommand{\b}{m}{\ensuremath{(}##1\ensuremath{)}}
	\DeclareDocumentCommand{\B}{m}{\ensuremath{\bm{(}}##1\ensuremath{\bm{)}}}
	
