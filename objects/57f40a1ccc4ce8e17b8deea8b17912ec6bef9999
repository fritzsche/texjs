	\DeclareDocumentCommand{\rsrgarrow}{O{} O{\ensuremath{x}} O{\ensuremath{f}} O{}}{\rentry[##1]{\srgarrow[##2][##3]}[##4]}
	\ifempty{#1}\item\textit{\@fragolivarrulessyn}:\else\item[#1]\fi
	\begin{itemize}
	}
{\end{itemize}
}

