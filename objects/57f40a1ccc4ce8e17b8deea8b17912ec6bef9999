\DeclareDocumentCommand{\fglresetsituationspacing}{}{\renewcommand{\@fglssituationspacing}{\@fglssituationspacingbackup}}

\DeclareDocumentCommand{\sarg}{m}{\sobj{s}{#1}[\@fglssituationspacing]}
