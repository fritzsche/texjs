	\let\@rentrylabel\@fglsundefined
	\newcommand{\@rentrylabel}{R}
	\let\@rentrylabellower\@fglsundefined
	\newcommand{\@rentrylabellower}{r}
	\stepcounter{@fglscalcitemcounter}
	\stepcounter{@fglsderivationcounter}
