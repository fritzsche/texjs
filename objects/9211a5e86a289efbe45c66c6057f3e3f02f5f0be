%<package>% fi A.1.1.1
%<package>\fi
%<package>% else A.1.1
%<package>\else
%<package>\ispmpl{\treq.\quattroq\cinqueq\seiq\setteq}\ispmpr\ispmtenf$^{-1}$%
%<package>% fi A.1.1
