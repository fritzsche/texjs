    M0=A+u*(1.15,0);
    M1-M0=u*(0,0.375);
    M2-M1=u*(-0.125,0);
    M3-A=u*(-1.15,0.75);
    M4-A=u*(-1.15,0);
    path klaxon[];
    klaxon1=M0--M1--M2{dir-135}..{dir135}M3--M4;
    klaxon2=klaxon1--symetrie(reverse(klaxon1),A,M0)--cycle;
    fill klaxon2;
    trace pointarc(cdinterne,135)--pointarc(cdinterne,315) withpen pensquare scaled 8 withcolor red;
    MiseEchelle;
  \end{mplibcode}%
}%
