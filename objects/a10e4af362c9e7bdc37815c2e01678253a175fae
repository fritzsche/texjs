\DeclareDocumentCommand\opbraces{ m g o d() }
{
  \IfNoValueTF{#2}
  {
  \IfNoValueTF{#3}
  {
    \IfNoValueTF{#4}
    {#1}
    {\fbraces{\lparen}{\rparen}{#1}{#4}}
  }
  {
    \fbraces{\lbrack}{\rbrack}{#1}{#3}
    \IfNoValueTF{#4}{}{(#4)}
  }
  }
  {
  \fbraces{\lbrace}{\rbrace}{#1}{#2}
  \IfNoValueTF{#3}{}{[#3]}
  \IfNoValueTF{#4}{}{(#4)}
  }
}
\DeclareDocumentCommand\trigbraces{ m o d() }
{
  \IfNoValueTF{#3}
  {#1 \IfNoValueTF{#2}{}{[#2]}}
  {#1 \IfNoValueTF{#2}{}{^{#2}} \argopen(#3\argclose)}
}

