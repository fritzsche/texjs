% \begin{pspicture}(\@tempdimX,0)%
%  \rput[bl](0,0.86em){\hbox to\@tempdimX{\hss
%  \begin{tabular}{c}#4%
%  \end{tabular}\hss}}%
%  \Arrow@Element{\@tempdimX}%
%  \rput[tl](0,-0.3em){\hbox to\@tempdimX{\hss
%  \begin{tabular}{c}%
%  #5\end{tabular}\hss}}%
% \end{pspicture}\endgroup}}
