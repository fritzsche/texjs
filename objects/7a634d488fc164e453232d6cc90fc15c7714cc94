%==============================================================================
CAT:nucleic acid;EN:Adenine;MW:135.13;LV:-
=
  sw_frame:=Atom;
:
 <30,?6,3=?5,1'3'5'9=db,2'6'9:N,5:/NH2,7:NH
*
 defaultscale:=.5;
 label.bot(decimal(fig_num)&":"&EN,(.5w,0));
+
 defaultscale:=.3;
 label.bot("A2",A2) withcolor red;
 label.top("A6",A6) withcolor red;
 label.top("A9",A9) withcolor red;
;
