\input calcfrac.tex
\ProvidesPackage{\calcfracname}[\calcfracdate\space\calcfracver\space Calcul d'expressions avec fractions]
\def\cfdisp#1#2#3{%
	\ensuremath
		{%
		\ifnum#11=-1 -\fi
		\ifnum#3=1
			#2%
		\else
			\frac{#2}{#3}%
		\fi
		}%
}