\def \GenDate {2011/10/25} %% <- TODO update!
\ProvidesFile{makedots.tex}[\GenDate\space blogdot variants driver (UL)] 
 
\RequirePackage{blogdot,texlinks} 
\input{texblog.fdf} \input{dantev45.fdf}

\newcommand*{\makedantevvar}[1]{%
    \def\htmljob{dantev45-#1}%      %% for texblog ...
    \ResultFile{\htmljob.html}%
    \SumBlogDotWidth 
    \BlogCopyFile[\TextCodes 
                  \MakeActiveDef\"{\catchdq}%
                 ]{dantev45.tex}%
    \CloseResultFile}

\makedantevvar{992-exact}

\ShowBlogDotFrame
\makedantevvar{992-exact-frame}
\DontShowBlogDotFrame

\renewcommand*{\lowerpagemargin}{504}

\FillBlogDotTypeArea
\makedantevvar{992-com}

\renewcommand*{\leftpagemargin}{256}

\makedantevvar{1180-com}
\DontFillBlogDotTypeArea

\makedantevvar{1180-clean}

\renewcommand*{\leftpagemargin}{50}

\FillBlogDotTypeArea
\makedantevvar{768-com}
\DontFillBlogDotTypeArea

\renewcommand*{\lowerpagemargin}{\upperpagemargin}
\ShowBlogDotFrame
\makedantevvar{768-exact-frame}

\ShowBlogDotBorders
\makedantevvar{768-exact-show}

\FillBlogDotTypeArea \ShowBlogDotFillText
\makedantevvar{768-filltype-show}

\stop
