%    \begin{macrocode}
%<*option&(class|extend)>
\newcommand*{\scr@v@is@eq}[1]{%
  \scr@compatibility=\@nameuse{scr@v@#1}
}
%</option&(class|extend)>
%    \end{macrocode}
% \end{macro}
