%   \put(21.00,20.00){\makebox(0,0)[cc]{%
%       \shortstack{%
%           \ifnum\language=\languageNGerman\meta{Beschreibung\(_1\)}%
%           \else\meta{desc\(_1\)}\fi\\\texttt{\#2}}}}
%   \put(46.50,20.00){\makebox(0,0)[cc]{%
