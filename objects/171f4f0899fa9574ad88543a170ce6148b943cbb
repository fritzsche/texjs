\DeclareKeys{
  iso.if =           @nss@iso,
  mathbb.ifnot =     @nss@iso,
  mathitalics.if =   @nss@mathitalics,
  mathnormal.ifnot = @nss@mathitalics
}

\ProcessKeyOptions
\SetKeys{iso=false, mathitalics=true} % the default values for use with struktex.sty

\DeclareSymbolFont{italics}{\encodingdefault}{\rmdefault}{m}{it}%
\DeclareSymbolFont{AMSb}{U}{msb}{m}{n}
\DeclareSymbolFontAlphabet{\mathbb}{AMSb}
