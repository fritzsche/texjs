  \typeout{debug active}}{
  \typeout{debug inactive}
  \renewcommand{\dbgCond@nss}[1]{}
  \renewcommand{\dbgCount@nss}[2][]{}
  \renewcommand{\dbgCoord@nss}[3][]{}
  \renewcommand{\dbgMark@nss}[3][]{}
  \renewcommand{\dbgString@nss}[1]{}
}
