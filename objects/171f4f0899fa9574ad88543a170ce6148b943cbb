% \section{\ifnum\language=\languageNGerman%
%     Verschiedene Beispieldateien
% \else%
%     Several sample files
% \fi}
%
%
% \subsection[\ifnum\language=\languageNGerman%
% Test des Paketes \sFile{struktxf.sty}\else Testing
% the package \sFile{struktxf.sty}\fi]{%
% \ifnum\language=\languageNGerman%
%     Datei zum Austesten der Makros des Paketes \sFile{struktxf.sty}%
% \else%
%     File for testing the macros of the package \sFile{struktxf.sty}%
