\DeclareOption{pdf}{\def\pgfsysdriver{pgfsys-common-pdf.def}}
\DeclareOption{dvipdfm}{\def\pgfsysdriver{pgfsys-dvipdfm.def}}
\DeclareOption{pdftex}{\def\pgfsysdriver{pgfsys-pdftex.def}}
\DeclareOption{ps}{\def\pgfsysdriver{pgfsys-common-postscript.def}}
\DeclareOption{dvips}{\def\pgfsysdriver{pgfsys-dvips.def}}
\ExecuteOptions{dvipdfm}
\InputIfFileExists{pict2e.cfg}{}{}
\ProcessOptions\relax
